URxvt.internalBorder: 100
! special
*.foreground:   #a2a2a2
*.background:   #292929
*.cursorColor:  #a2a2a2

!!*background: rgba(0, 0, 0, .7)

! black
*.color0:       #4e4e4e
*.color8:       #777777

! red
*.color1:       #a9635d
*.color9:       #ffbbb2

! green
*.color2:       #468459
*.color10:      #9ddeaf

! yellow
*.color3:       #84763d
*.color11:      #e0ce91

! blue
*.color4:       #5879af
*.color12:      #b3d1ff

! magenta
*.color5:       #9c6594
*.color13:      #fabdf0

! cyan
*.color6:       #008592
*.color14:      #79e0ed

! white
*.color7:       #cfcfcf
*.color15:      #ffffff

URxvt.intensityStyles:  false
URxvt.background:       [70]#000000
URxvt.depth:            32
URxvt.font:             xft:Hack:size=14,xft:Symbola:size:=14
URxvt.scrollBar:        false
URxvt.cursorColor:      white

!! Extensions
URxvt.perl-ext-common:  default,matcher,resize-font,url-select,keyboard-select
URxvt.colorUL:          #4682B4
!! url-select
URxvt.keysym.M-u:       perl:url-select:select_next
URxvt.url-select.launcher: linkhandler
URxvt.url-select.underline: true
!! keyboard-select:
URxvt.keysym.M-Escape: perl:keyboard-select:activate
!! resize-font
URxvt.resize-font.smaller:      C-Down
URxvt.resize-font.bigger:       C-Up
!! Matcher
URxvt.url-launcher:     linkhandler
URxvt.matcher.button:   1

rofi.color-enabled:     true
rofi.color-window:      #292929, #292929, #292929
rofi.color-normal:      #111, #819396, #222, #008ed4, #ffffff
rofi.color-active:      #002b37, #008ed4, #003643, #008ed4, #66c6ff
rofi.color-urgent:      #002b37, #da4281, #003643, #008ed4, #890661

rofi.fake-transparency: true
rofi.lines:             3
rofi.bw:                0
rofi.opacity:           "10"
rofi.hide-scrollbar:    true
rofi.width:             20

